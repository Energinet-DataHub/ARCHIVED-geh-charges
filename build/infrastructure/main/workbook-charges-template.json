{
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workbookDisplayName": {
      "type": "string",
      "defaultValue": "${workbook_display_name}",
      "metadata": {
        "description": ""
      }
    },
    "workbookId": {
      "type": "string",
      "defaultValue": "${workbook_id}",
      "metadata": {
        "description": "The unique guid for this workbook instance"
      }
    },
    "subscriptionId": {
      "type": "string",
      "defaultValue": "${subscription_id}",
      "metadata": {
        "description": ""
      }
    }
	,
    "resourceGroup": {
      "type": "string",
      "defaultValue": "${resouce_group_name}",
      "metadata": {
        "description": ""
      }
    }
    ,
    "sharedResourceGroup": {
      "type": "string",
      "defaultValue": "${shared_resouce_group_name}",
      "metadata": {
        "description": ""
      }
    }
    ,
    "applicationInsightName": {
      "type": "string",
      "defaultValue": "${application_insight_name}",
      "metadata": {
        "description": ""
      }
    },
    "sourceId": {
      "type": "string",
      "defaultValue": "[concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('sharedResourceGroup'), '/providers/Microsoft.Insights/components/', parameters('applicationInsightName'))]",
      "metadata": {
        "description": ""
      }
    },
    "workbookComponents": {
      "type": "string",
      "defaultValue": "${workbook_components}"
    }
  }, 
  "variables": {
    "workbookContent": {
      "version": "Notebook/1.0",
      "items": "[parameters(workbookComponents)]",
      "isLocked": false
    }
  },
  "resources": [
    {
      "name": "[parameters('workbookId')]",
      "type": "microsoft.insights/workbooks",
      "location": "[resourceGroup().location]",
      "apiVersion": "2021-03-08",
      "dependsOn": [],
      "kind": "shared",
      "properties": {
        "displayName": "[parameters('workbookDisplayName')]",
        "serializedData": "[string(variables('workbookContent'))]",
        "version": "1.0",
        "sourceId": "[parameters('sourceId')]",
        "category": "workbook"
      }
    }
  ],
  "outputs": {
    "workbookId": {
      "type": "string",
      "value": "[resourceId( 'microsoft.insights/workbooks', parameters('workbookId'))]"
    }
  },
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#"
}